{
  "extends": "./.config/tsconfig.json",
  "compilerOptions": {
    "alwaysStrict": false,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "strict": false,    
    "strictFunctionTypes": false,
    "strictBindCallApply": false,
    "strictPropertyInitialization": false,
    "noImplicitReturns": false,
    "paths": {
      "@/*": [
        "*"
      ]
    }
  }
}